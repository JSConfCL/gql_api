{
  "name": "worker-apollo-server",
  "main": "./build/index.js",
  "scripts": {
    "drizzle-kit": "drizzle-kit",
    "db:generate": "drizzle-kit generate:sqlite && prettier ./drizzle/migrations --write",
    "db:check": "drizzle-kit check:sqlite",
    "db:up": "drizzle-kit up:sqlite",
    "db:studio": "drizzle-kit studio --verbose",
    "db:migrate": "tsx migrate.ts",
    "db:model": "tsx ./drizzle/dbml.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:interactive": "vitest --ui --coverage",
    "test:ci": "vitest --coverage",
    "lint": "eslint ./src --cache",
    "prettier": "prettier ./src --cache",
    "prettier:ci": "prettier ./src --cache --check",
    "prettier:fix": "prettier ./src --cache --write",
    "typecheck": "tsc",
    "dev": "wrangler dev --compatibility-date 2022-11-30 --log-level info --port 50123",
    "dev:wall-of-fame": "wrangler dev --test-scheduled --config ./workers/wall_of_fame_cron/wrangler.toml",
    "dev:auth": "wrangler dev --test-scheduled --config ./workers/auth_tokens/wrangler.toml",
    "generate": "tsx ./src/generated/generate.ts && graphql-codegen --config codegen.ts && prettier ./src/generated --write",
    "email:dev": "email dev",
    "email:export": "email export"
  },
  "devDependencies": {
    "@clerk/types": "^3.48.1",
    "@cloudflare/workers-types": "^4.20230724.0",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typed-document-node": "^5.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.2.0",
    "@react-email/components": "0.0.6",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "better-sqlite3": "^8.7.0",
    "dotenv": "^16.3.1",
    "drizzle-dbml-generator": "^0.4.0",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "graphql-tag": "^2.12.6",
    "prettier": "^3.0.0",
    "react-email": "1.9.3",
    "sqlite3": "^5.1.6",
    "tsx": "^3.13.0",
    "type-fest": "^4.6.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0",
    "wrangler": "^3.15.0"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@clerk/backend": "^0.26.0",
    "@clerk/edge": "^1.13.2",
    "@cloudflare/workers-honeycomb-logger": "^2.3.3",
    "@envelop/core": "^4.0.0",
    "@envelop/immediate-introspection": "^2.0.0",
    "@envelop/opentelemetry": "^5.0.0",
    "@envelop/rate-limiter": "^5.0.0",
    "@faker-js/faker": "^8.0.2",
    "@graphql-authz/envelop-plugin": "^1.0.4",
    "@graphql-yoga/plugin-csrf-prevention": "^2.0.3",
    "@libsql/client": "^0.3.5",
    "@microlabs/otel-cf-workers": "^1.0.0-rc.6",
    "@neondatabase/serverless": "^0.4.26",
    "@opentelemetry/sdk-trace-base": "^1.15.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-authz": "^3.5.8",
    "@sanity/client": "^6.7.0",
    "@types/react": "^18.2.22",
    "cookie": "^0.5.0",
    "dbdocs": "^0.8.2",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.5.0",
    "graphql": "^16.7.1",
    "graphql-config": "^5.0.2",
    "graphql-scalars": "^1.22.2",
    "graphql-yoga": "^4.0.3",
    "hono": "^3.9.0",
    "libhoney": "^4.0.1",
    "nexus": "^1.3.0",
    "p-map": "^6.0.0",
    "react": "^18.2.0",
    "slugify": "^1.6.6",
    "stripe": "^14.2.0",
    "worker-auth-providers": "^0.0.13-beta.4",
    "zod": "^3.21.4"
  }
}
